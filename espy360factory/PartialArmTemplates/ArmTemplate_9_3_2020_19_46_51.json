{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "espy360factory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Copy Top n Procs')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "espy360blob1//MPUPD_Top_N_Procs.csv"
							},
							{
								"name": "Destination",
								"value": "mpupd_top_n_procs"
							}
						],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "AzureMySqlSink",
								"writeBatchSize": 10000,
								"writeBatchTimeout": "00:00:30"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "national_provider_identifier",
											"type": "Int32",
											"physicalType": "String"
										},
										"sink": {
											"name": "national_provider_identifier",
											"type": "Int32",
											"physicalType": "int"
										}
									},
									{
										"source": {
											"name": "hcpcs_code",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "hcpcs_code",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "year",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "year",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "proc_count",
											"type": "Int32",
											"physicalType": "String"
										},
										"sink": {
											"name": "proc_count",
											"type": "Int32",
											"physicalType": "int"
										}
									},
									{
										"source": {
											"name": "rank",
											"type": "Int32",
											"physicalType": "String"
										},
										"sink": {
											"name": "rank",
											"type": "Int32",
											"physicalType": "int"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "MPUDP_Top_N_Procs",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "top_n_table",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/MPUDP_Top_N_Procs')]",
				"[concat(variables('factoryId'), '/datasets/top_n_table')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/MPUDP_Top_N_Procs')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureBlobStorage1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "MPUPD_Top_N_Procs.csv",
						"container": "espy360blob1"
					},
					"columnDelimiter": "|",
					"rowDelimiter": "\n",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "national_provider_identifier",
						"type": "String"
					},
					{
						"name": "hcpcs_code",
						"type": "String"
					},
					{
						"name": "year",
						"type": "String"
					},
					{
						"name": "proc_count",
						"type": "String"
					},
					{
						"name": "rank",
						"type": "String"
					}
				]
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/top_n_table')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureMySql1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureMySqlTable",
				"schema": [
					{
						"name": "national_provider_identifier",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "hcpcs_code",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "year",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "proc_count",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "rank",
						"type": "int",
						"precision": 0,
						"scale": 0
					}
				],
				"typeProperties": {
					"tableName": "mpupd_top_n_procs"
				}
			},
			"dependsOn": []
		}
	]
}