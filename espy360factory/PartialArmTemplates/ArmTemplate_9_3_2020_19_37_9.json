{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "espy360factory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/mpupd_top_n_prcs_table')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureMySql1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureMySqlTable",
				"schema": [
					{
						"name": "national_provider_identifier",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "hcpcs_code",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "year",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "proc_count",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "rank",
						"type": "int",
						"precision": 0,
						"scale": 0
					}
				],
				"typeProperties": {
					"tableName": "mpupd_top_n_procs"
				}
			},
			"dependsOn": []
		}
	]
}